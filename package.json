{
  "name": "urlmage",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.15.2",
  "description": "URL manipulation library for TypeScript",
  "author": "outslept",
  "license": "MIT",
  "homepage": "https://github.com/outslept/urlmage#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/outslept/urlmage.git"
  },
  "bugs": {
    "url": "https://github.com/outslept/urlmage/issues"
  },
  "keywords": [
    "url",
    "uri"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "unbuild",
    "build:watch": "unbuild --stub",
    "clean": "node -e \"fs.rmSync('dist', { recursive: true, force: true })\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:inspect": "nlx eslint-config-inspector",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "nr build",
    "validate": "concurrently -g \"nr typecheck\" \"nr lint\" \"nr test\"",
    "release": "nr validate && nr build && changeset publish",
    "release:dry": "nr validate && nr build && changeset version",
    "prepare": "simple-git-hooks",
    "deps": "taze",
    "deps:major": "taze major",
    "deps:minor": "taze minor",
    "deps:patch": "taze patch",
    "deps:update": "taze -w",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "nanoid": "5.0.9",
    "p-memoize": "7.1.1",
    "p-queue": "8.0.1",
    "parse-domain": "8.2.2",
    "pino": "9.6.0",
    "pino-http": "10.3.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@antfu/ni": "^0.21.12",
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@types/node": "22.13.6",
    "@vitest/coverage-v8": "2.1.8",
    "concurrently": "^8.2.2",
    "eslint": "^9.21.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "15.3.0",
    "pathe": "2.0.3",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.9",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "unbuild": "^3.5.0",
    "vitest": "2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx --no -- commitlint --edit $1"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
